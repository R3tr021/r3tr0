public class Node
{
    private Zahlobjekt meinObjekt;
    private Node folgenderNode;
    
    public Zahlobjekt getMeinObjekt()
    {
        return meinObjekt;
    }
    public Node getFolgenderNode()
    {
        return folgenderNode;
    }
    public void setMeinObjekt(Zahlobjekt p)
    {
        meinObjekt = p;
    }
    public void setFolgenderPlatz(Node p)
    {
        folgenderNode = p;
    }
}
